<UserControl x:Class="VkWpfPlayer.EISDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VkWpfPlayer"
        mc:Ignorable="d"
         Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label x:Name="ContentData" Content="{Binding ContentDialogData}"  >
                
            </Label>
            <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
                <Button x:Name="AcceptButton" Width="80"  Style="{DynamicResource ButtonStyleRect}"  Click="Accept_Click" Content="Да"> </Button>
                <Button x:Name="CancelButton" Width="80" Style="{DynamicResource ButtonStyleRect}" Margin="10,0,0,0" Click="Cancel_Click" Content="Отменить"></Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
